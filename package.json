{
  "name": "by-node-env",
  "version": "1.3.0",
  "description": "Run package.json scripts by NODE_ENV",
  "keywords": [
    "cross-per-env",
    "development",
    "dotenv",
    "env",
    "if-env",
    "node",
    "node-env",
    "npm",
    "nps",
    "nps-utils",
    "package-json",
    "per-env",
    "pnpm",
    "production",
    "run",
    "run-script",
    "scripts",
    "test",
    "yarn",
    "yo-env"
  ],
  "homepage": "https://github.com/shian15810/by-node-env#readme",
  "bugs": "https://github.com/shian15810/by-node-env/issues",
  "license": "MIT",
  "author": "Chuah Chee Shian <shian15810@gmail.com>",
  "files": [
    "dist/index.*"
  ],
  "main": "dist/index.js",
  "bin": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "dist/index.mjs",
  "source": "src/index.ts",
  "umd:main": "dist/index.umd.js",
  "repository": "https://github.com/shian15810/by-node-env.git",
  "scripts": {
    "format": "prettier --write \"**/*\"",
    "build": "rimraf dist && microbundle build --strict --target node",
    "test": "node dist && cross-env NODE_ENV=test node dist",
    "pretest:development": "node test.js",
    "test:development": "node test.js",
    "posttest:development": "node test.js",
    "pretest:test": "node test.js",
    "test:test": "node test.js",
    "posttest:test": "node test.js",
    "prepublishOnly": "npm run test"
  },
  "dependencies": {
    "commander": "^2.20.0",
    "commander-remaining-args": "^1.2.0",
    "dotenv": "^8.0.0",
    "execa": "^2.0.3",
    "preferred-pm": "^2.0.0",
    "read-pkg-up": "^6.0.0",
    "which-pm-runs": "^1.0.0"
  },
  "devDependencies": {
    "@types/commander-remaining-args": "^1.2.0",
    "@types/dotenv": "^6.1.1",
    "@types/node": "^12.6.8",
    "cross-env": "^5.2.0",
    "get-bin-path": "^1.0.2",
    "husky": "^3.0.2",
    "microbundle": "^0.11.0",
    "prettier": "^1.18.2",
    "pretty-quick": "^1.11.1",
    "rimraf": "^2.6.3",
    "typescript": "^3.5.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
